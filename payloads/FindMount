USE_ECM=false
USE_RNDIS=false

USE_HID=true
HID_KEYBOARD_TEST=true
lang="us"

function onKeyboardUp()
{
cat <<- EOF | duckhid
DELAY 500
GUI r
DELAY 200
STRING notepad.exe
ENTER
DELAY 300
STRING cd /d "%TEMP%"
ENTER
STRING erase /Q DuckyWait.bat
ENTER
STRING copy con DuckyWait.bat
ENTER
EOF

echo "echo list volume > \"%TEMP%\diskpart.srp\"" | outhid
echo "echo exit >> \"%TEMP%\diskpart.srp\"" | outhid
echo "diskpart /s \"%TEMP%\diskpart.srp\" | findstr \"DUCKY\" > \"%TEMP%\out.srp\"" | outhid
echo "set /p DriveTemp= < \"%TEMP%\out.srp\"" | outhid
echo "set Drive=%DriveTemp:~15,1%" | outhid
echo "if \"%Drive%\" == \" \" echo \"Ducky drive is not mounted.\"" | outhid

cat <<- EOF | duckhid
STRING CONTROL z
ENTER
EOF
}
